<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://xmlns.jcp.org/jsf/html"
	xmlns:f="http://xmlns.jcp.org/jsf/core"
	xmlns:p="http://primefaces.org/ui"
	xmlns:ui="http://xmlns.jcp.org/jsf/facelets">

<h:head>

</h:head>

<h:body>
	<h:outputStylesheet name="css/style.css" target="body" />
	<h:form>

		<p:spinner />

		<p:inputText value="#{testAction.aa}" />

		<p:commandButton value="testAction.hello" icon="ui-icon-star"
			actionListener="#{testAction.hello2}" update="@form checkboxDT">
		</p:commandButton>
		<p:commandButton value="gaojian" icon="ui-icon-star"
			actionListener="#{testAction.hello}">
		</p:commandButton>
		<p:outputLabel for="inline" value="Inline:" />
		<p:calendar id="inline" />
		<input type="file" capture="camera" accept="image/*" id="cameraInput"
			name="cameraInput" class="sign_file" />

		<p:growl id="messages" showDetail="true" />

		<p:fileUpload value="#{testAction.file}" mode="simple"
			skinSimple="true" />

		<p:commandButton value="Submit" ajax="false"
			actionListener="#{testAction.upload}" disabled="false" />
		<p:dataTable id="checkboxDT" var="customer"
			value="#{testAction.model}" selection="#{testAction.selected}"
			rowKey="#{customer.custId}" style="margin-bottom:0">
			<f:facet name="header">
            Checkbox
        	</f:facet>
			<p:column selectionMode="multiple"
				style="width:16px;text-align:center" />
			<p:column headerText="Id">
				<h:outputText value="#{customer.custId}" />
			</p:column>
			<p:column headerText="Id">
				<h:outputText value="#{customer.custId}" />
			</p:column>
			<p:column headerText="Year">
				<h:outputText value="#{customer.custName}" />
			</p:column>
			<p:column headerText="Brand">
				<h:outputText value="#{customer.telNo}" />
			</p:column>
		</p:dataTable>
		
		<p:selectOneMenu id="car" value="#{testAction.selectValue}">
            <f:selectItems value="#{masterService.getNameCd('A')}" />
        </p:selectOneMenu>
        <p:selectOneMenu id="car2" value="#{testAction.selectValue}">
            <f:selectItems value="#{masterService.getNameCd('B')}" />
        </p:selectOneMenu>
        <p:selectOneMenu id="advanced" value="#{testAction.selectValue}"  panelStyle="width:180px"
                         effect="fade" var="t" style="width:160px" filter="true" filterMatchMode="contains">
            <f:selectItems value="#{masterService.getItemList()}" var="item" itemLabel="#{item.getLabel}" itemValue="#{item.getValue}" />
 
            <p:column>
                <h:outputText value="#{item.getLabel}" />
            </p:column>
        </p:selectOneMenu>
		
	</h:form>

	<h:form>
		<p:fileUpload fileUploadListener="#{fileUploadView.handleFileUpload}"
			mode="advanced" dragDropSupport="false" update="messages"
			fileLimit="3" allowTypes="/(\.|\/)(gif|jpe?g|png)$/" />

		<p:growl id="messages" showDetail="true" />

	</h:form>

</h:body>
</html>
