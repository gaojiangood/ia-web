select
  PO_ID,
  PO_DATE,
  PO_NAME,
  CUST_ID,
  ITEM_PRICE_SUM,
  PRICE_CUT,
  PRE_PAY,
  SHIP_FEE,
  OTHER_FEE,
  QTY_SUM,
  ITEM_COST_SUM,
  PROFIT_SUM,
  PO_STATE,
  COMMENT,
  REG_USER_ID,
  REG_TIME,
  UPD_USER_ID,
  UPD_TIME,
  VERSION_NO
from
  T_PO
where
  PO_ID = /* poId */'a'
  and
  VERSION_NO = /* versionNo */1
