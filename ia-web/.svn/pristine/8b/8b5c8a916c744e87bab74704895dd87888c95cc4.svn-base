<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://xmlns.jcp.org/jsf/html"
	xmlns:f="http://xmlns.jcp.org/jsf/core"
	xmlns:p="http://primefaces.org/ui"
	xmlns:ui="http://xmlns.jcp.org/jsf/facelets">

<h:head>
	<title>一键装箱</title>
	<style type="text/css">
.ui-widget {
	font-size: 90%;
}
</style>
</h:head>

<h:body>

	<h:form>
		<h:outputText id="shipId" value="请选择装箱单" />
		<p:selectOneMenu value="#{poModel.fillShipId}" style="width:200">
			<f:selectItems value="#{masterService.getShipList()}" />
		</p:selectOneMenu>
		<p:commandButton value="确定" process="@form"
			actionListener="#{poAction.closeFillShipId}" />
	</h:form>
</h:body>
</html>
